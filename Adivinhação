print("**********************************")
print("Bem vindo ao jogo de Adivinhação!")
print("**********************************")

numero_secreto = 42

chute_str = input("Digite o seu número: ")

print("você digitou ", chute_str)

chute = int(chute_str)

if numero_secreto == chute:
    print("voce acertou")
else:
    print("você errou!")

print("Fim do jogo")
